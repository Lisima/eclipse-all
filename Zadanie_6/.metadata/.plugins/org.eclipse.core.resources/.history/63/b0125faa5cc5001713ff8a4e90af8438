import java.util.Scanner;

public class Suma {

	public static void main(String[] args) 
	{
		for(int i=0; i<=10; i++)
		{
			Scanner scanner = new Scanner(System.in);
			
			System.out.println("Podaj Liczbê " );
			int a = scanner.nextInt();
			int suma = 0;
			suma = suma + a;
			
			
		}
		String suma;
		System.out.println("Wynik to " +suma );
	}

}
